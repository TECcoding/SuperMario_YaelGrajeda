.main-container {
    flex-direction: column;
    justify-content: center;
    align-items: center;
    background-color: #5f7ed6;
    width: 100%;
    height: 100%;
}

.regresa-button {
    width: 200px;
    height: 60px;
    font-size: 24px;
    font-weight: bold;
    color: white;
    background-color: #f76b1c;
    border-radius: 20px;
    transition: transform 0.2s ease-in-out;
}

.regresa-button:hover {
    transform: scale(1.1);
}
